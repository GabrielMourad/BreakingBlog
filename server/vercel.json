{
    "version": 2,
    "builds": [
      { "src": "*.js", "use": "@vercel/node" }
    ],
    "routes": [
        { 
            "src": "/(.*)", 
            "dest": "/", 
            "headers": {
                "Access-Control-Allow-Origin": "https://breaking-blog-server.vercel.app/login",
                "Access-Control-Allow-Methods": "GET, POST",
                "Access-Control-Allow-Headers": "Content-Type"
            }
        }
    ]
}
